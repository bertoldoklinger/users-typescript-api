{
  "name": "typescript-api-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start:build": "node dist/server.js",
    "start": "nodemon --watch 'src/' --exec ts-node-esm -r tsconfig-paths/register src/index.ts -e ts",
    "husky:prepare": "husky install",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --passWithNoTests --watch",
    "test:staged": "jest --passWithNoTests --findRelatedTests",
    "test:push": "jest --passWithNoTests --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.5",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.6",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.4",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.0.2"
  },
  "lint-staged": {
    "src/*.{ts,tsx}": [
      "eslint . --fix",
      "prettier --write ."
    ]
  }
}
